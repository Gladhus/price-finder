  <div class="search-content">
    <h1>{{title}}</h1>
    <p>Welcome to {{title}}</p>

    <div class="PFBox PFMain">
      <h5>Find the average price of an item</h5>
      <div class="form-horizontal">
        <div class="form-group">
          <label for="keywords" class="control-label col-xs-2">Keywords</label>
          <div class="col-sm-9 tagsinput-primary js-keywords">
            <input type="text" name="keywords" class="form-control PFInput" data-role="tagsinput">
          </div>
        </div>

        <div class="form-group">
          <label for="tags" class="control-label col-xs-2">Tags</label>
          <div class="col-sm-9 tagsinput-primary js-tags">
            <input type="text" name="tags" class="form-control PFInput tagsinput" data-role="tagsinput">
          </div>
        </div>

        <div class="form-group">
          <label for="categories" class="control-label col-xs-2">Category</label>
          <div class="col-sm-9">
            <select data-toggle="select" class="form-control select select-primary select-block mbl PFSelect js-category">
              <optgroup label="Cars & vehicles">
                <option value="0">Cars</option>
                <option value="1">Motorcycless</option>
                <option value="2">Boats, watercrafts</option>
                <option value="3">AVTs, snowmobiles</option>
              </optgroup>
              <optgroup label="Computers">
                <option value="4">Desktop computers</option>
                <option value="5">iPads, tablets</option>
                <option value="6">Laptops</option>
                <option value="7">Servers</option>
              </optgroup>
            </select>
          </div>
        </div>

        <div class="form-group">
          <label for="targets" class="control-label col-xs-2">Targets</label>
          <div class="col-sm-9">
            <select multiple="multiple" name="targets" class="form-control multiselect multiselect-primary js-target">
              <option value="Kijiji">Kijiji</option>
              <option value="Craigslist">Craigslist</option>
              <option value="eBay">eBay</option>
              <option value="Amazon">Amazon</option>
              <option value="Alibaba">Alibaba</option>
            </select>
          </div>
        </div>

        <div class="form-group last">
          <div class="col-sm-11">
            <button class="btn btn-primary btn-wide PFSearchBtn" onClick="buildRequest()">Search the item price</button>
          </div> <!-- end of col-sm-11 -->
        </div> <!-- end of form-group -->
      </div> <!-- end of form-horizontal -->
    </div> <!-- end of PFBox -->
  </div> <!-- end of search-content -->

